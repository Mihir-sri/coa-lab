#include <iostream>
using namespace std;

int main() {
    int a,b;
    cin>>a>>b;
	cout<<"a = " << a <<","<< " b = " << b <<endl;
	cout << "Bitwise Operators\n";
    cout << "a & b = " << (a&b) << "\n";
    cout << "a | b = " << (a|b) << "\n";
    cout << "a ^ b = " << (a^b) << "\n";
    cout << "~a = " << (~a) << "\n";
    cout << "~b = " << (~b) << "\n";
    cout << "a >> b = " << (a>>b) << "\n";
    cout << "a << b = " << (a<<b) << "\n";
	return 0;
}


output
a = 5, b = 2
Bitwise Operators
a & b = 0
a | b = 7
a ^ b = 7
~a = -6
~b = -3
a >> b = 1
a << b = 20




